{
  "name": "usd-query",
  "version": "2.0.0",
  "description": "Just make $ alias for document.querySelector.",
  "repository": {
    "type": "git",
    "url": "https://github.com/laststance/usd-query"
  },
  "license": "MIT",
  "author": "Ryota Murakami <dojce1048@gmail.com> (https://github.com/ryota-murakami)",
  "type": "module",
  "main": "./lib/index.js",
  "sideEffects": false,
  "files": [
    "bin/",
    "lib/",
    "package.json",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier \"**/*\" --ignore-unknown",
    "format:write": "pnpm format --write",
    "lint": "npm-run-all --parallel lint:*",
    "lint:fix": "eslint . --ext .js,.cjs,.ts --fix",
    "lint:knip": "knip",
    "lint:spelling": "cspell \"**\" \".github/**/*\"",
    "lint:types": "tsc --noEmit",
    "test": "vitest",
    "release": "release-it"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitest/coverage-istanbul": "^0.34.2",
    "console-fail-test": "^0.2.3",
    "cspell": "^7.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-ts-prefixer": "^0.25.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.4.6",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-vitest": "^0.2.8",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "knip": "2.19.11",
    "lint-staged": "^14.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "release-it": "^16.1.5",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "node": ">=18"
  },
  "volta": {
    "node": "18.17.1"
  },
  "publishConfig": {
    "provenance": true
  }
}
