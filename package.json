{
	"name": "usd-query",
	"version": "0.0.0",
	"description": "Just make $ alias for document.querySelector.",
	"repository": {
		"type": "git",
		"url": "https://github.com/laststance/usd-query"
	},
	"license": "MIT",
	"author": "Ryota Murakami <dojce1048@gmail.com> (https://github.com/ryota-murakami)",
	"type": "module",
	"main": "./lib/index.js",
	"files": [
		"bin/",
		"lib/",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier \"**/*\" --ignore-unknown",
		"format:write": "pnpm format --write",
		"lint": "eslint . .*js --max-warnings 0 --report-unused-disable-directives",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:package": "npmPkgJsonLint .",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky install",
		"should-semantic-release": "should-semantic-release --verbose",
		"test": "vitest"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"devDependencies": {
		"@types/eslint": "^8.44.2",
		"@types/node": "^20.5.1",
		"@typescript-eslint/eslint-plugin": "^6.4.0",
		"@typescript-eslint/parser": "^6.4.0",
		"@vitest/coverage-istanbul": "^0.34.2",
		"console-fail-test": "^0.2.3",
		"cspell": "^7.0.0",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-deprecation": "^1.5.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsdoc": "^46.4.6",
		"eslint-plugin-jsonc": "^2.9.0",
		"eslint-plugin-markdown": "^3.0.1",
		"eslint-plugin-n": "^16.0.1",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"eslint-plugin-perfectionist": "^1.5.1",
		"eslint-plugin-regexp": "^1.15.0",
		"eslint-plugin-vitest": "^0.2.8",
		"eslint-plugin-yml": "^1.8.0",
		"husky": "^8.0.3",
		"jsonc-eslint-parser": "^2.3.0",
		"knip": "2.19.5",
		"lint-staged": "^14.0.0",
		"markdownlint": "^0.30.0",
		"markdownlint-cli": "^0.35.0",
		"npm-package-json-lint": "^7.0.0",
		"npm-package-json-lint-config-default": "^6.0.0",
		"prettier": "^3.0.2",
		"prettier-plugin-curly": "^0.1.2",
		"prettier-plugin-packagejson": "^2.4.5",
		"release-it": "^16.1.5",
		"sentences-per-line": "^0.2.1",
		"should-semantic-release": "^0.1.1",
		"tsup": "^7.2.0",
		"typescript": "^5.1.6",
		"vitest": "^0.34.2",
		"yaml-eslint-parser": "^1.2.2"
	},
	"packageManager": "pnpm@8.6.12",
	"engines": {
		"node": ">=18"
	},
	"volta": {
		"node": "18.17.1"
	},
	"publishConfig": {
		"provenance": true
	}
}
